Algoritmo "Consumo"

Var
      consumo, distancia, combustivel : real
      


Inicio
       escreva("Distancia percorrida: ")
       leia(distancia)
       
       escreva("Combustível gasto: ")
       leia(combustivel)
       
       consumo <- distancia / combustivel
       
       escreval("Consumo medio = ", consumo:5:3)


Fimalgoritmo